{
  "name": "f97/filter-list",
  "description": "DNS block list that can be used for AdGuardHome. (Does not work with Pi-hole)",
  "homepage": "f97/filter-list",
  "license": "GPLv3",
  "sources": [
    {
      "name": "OISD Big List",
      "source": "https://github.com/sjhgvr/oisd/raw/main/oisd_big.txt",
      "type": "adblock",
      "transformations": ["RemoveComments", "Compress", "Validate"]
    },
    {
      "name": "VNM: ABPVN List",
      "source": "https://raw.githubusercontent.com/abpvn/abpvn/master/filter/abpvn.txt",
      "type": "hosts",
      "transformations": ["RemoveComments", "Compress", "Validate"]
    },
    {
      "name": "bigdargon - hostsVN",
      "source": "https://raw.githubusercontent.com/bigdargon/hostsVN/master/hosts",
      "type": "hosts",
      "transformations": ["RemoveComments", "Compress", "Validate"]
    },
    {
      "name": "NoCoin Filter List",
      "source": "https://raw.githubusercontent.com/hoshsadiq/adblock-nocoin-list/master/hosts.txt",
      "type": "hosts",
      "transformations": ["RemoveComments", "Compress", "Validate"]
    },
    {
      "name": "CoinBlockerLists",
      "source": "https://zerodot1.gitlab.io/CoinBlockerLists/hosts_browser",
      "type": "hosts",
      "transformations": ["RemoveComments", "Compress", "Validate"]
    },
    {
      "name": "Easylist Cookie List",
      "source": "https://secure.fanboy.co.nz/fanboy-cookiemonster.txt",
      "type": "hosts",
      "transformations": ["RemoveComments", "Compress", "Validate"]
    }
  ],
  "transformations": [
    "RemoveComments",
    "Validate",
    "Deduplicate",
    "TrimLines",
    "InsertFinalNewLine",
    "RemoveEmptyLines"
  ]
}
